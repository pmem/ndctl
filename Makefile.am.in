EXTRA_DIST =
CLEANFILES =

AM_MAKEFLAGS = --no-print-directory

AM_CPPFLAGS = \
	-include $(top_builddir)/config.h \
	-DSYSCONFDIR=\""$(sysconfdir)"\" \
	-DLIBEXECDIR=\""$(libexecdir)"\" \
	-DPREFIX=\""$(prefix)"\" \
	-DNDCTL_MAN_PATH=\""$(mandir)"\" \
	-I${top_srcdir}/ \
	$(KMOD_CFLAGS) \
	$(UDEV_CFLAGS) \
	$(UUID_CFLAGS) \
	$(JSON_CFLAGS)

AM_CFLAGS = ${my_CFLAGS} \
	-fvisibility=hidden \
	-ffunction-sections \
	-fdata-sections

AM_LDFLAGS = \
	-Wl,--gc-sections \
	-Wl,--as-needed

SED_PROCESS = \
	$(AM_V_GEN)$(MKDIR_P) $(dir $@) && $(SED) \
	-e 's,@VERSION\@,$(VERSION),g' \
	-e 's,@prefix\@,$(prefix),g' \
	-e 's,@exec_prefix\@,$(exec_prefix),g' \
	-e 's,@libdir\@,$(libdir),g' \
	-e 's,@includedir\@,$(includedir),g' \
	< $< > $@ || rm $@

LIBNDCTL_CURRENT=26
LIBNDCTL_REVISION=1
LIBNDCTL_AGE=20

LIBDAXCTL_CURRENT=7
LIBDAXCTL_REVISION=0
LIBDAXCTL_AGE=6

LIBCXL_CURRENT=1
LIBCXL_REVISION=0
LIBCXL_AGE=0
